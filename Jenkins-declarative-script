pipeline
{
    agent any
    
    environment{
    PATH="/opt/apache-maven/bin:$PATH"
    }
    
    stages{
      
       stage("Git Checkout"){
            steps{
               echo "Git Checkout Successfully"
               /*
               git credentialsId: 'Git', url: 'https://github.com/vijay-mg/VIJAY_INFO'
               */
                 }
        } 
        
        stage("Maven Build"){
        steps{
           sh "mvn clean package"
             }
           }
       
    }
    
}
